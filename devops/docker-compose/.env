# Time taken in minutes for the API metrics in Azure API Management Service to sync
AZURE_MERTICS_SYNC_BUFFER_INTERVAL_MINUTES=15

# Method in which metrics is collected (requests or insights)
AZURE_METRICS_BY_REQUESTS_OR_INSIGHTS=requests

# Azure Agent Configurations
APICP_PUBLISH_ASSETS=true
