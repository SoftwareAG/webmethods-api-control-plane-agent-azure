APICP_ASSETS_SYNC_INTERVAL_SECONDS=300
APICP_METRICS_SEND_INTERVAL_SECONDS=900

# Runtime Configurations
APICP_RUNTIME_CAPACITY_VALUE=500000
APICP_RUNTIME_CAPACITY_UNIT=PER_YEAR
